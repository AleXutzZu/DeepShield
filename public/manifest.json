{
  "name": "DeepShield",
  "description": "Detect possible deepfake content on the web",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-circle.png"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"]
}